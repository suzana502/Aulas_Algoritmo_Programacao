algoritmo "Tabuada 2"

var

   contador, contador1, num: inteiro

inicio

  para contador1 <- 1 ate 3 faca
      escreva ("Digite um numero: ")
      leia(num)
      
      para contador <- 0 ate 10 faca
           escreval (num," x", contador, " =",num*contador)
      fimpara
  fimpara

fimalgoritmo
